<%- model_class = TechServ -%>
<div class="container">
    <div class="panel">
        <div class="panel-heading">
            <div class="panel-title">技术服务详情</div>
        </div>
        <div class="panel-body">
            <div>
                <dl class="dl-horizontal">
                    <dt><strong><%= model_class.human_attribute_name(:avatar) %>:</strong></dt>
                    <dd><img class="media-object" src="<%= @tech_serv.avatar %>" alt="Image"></dd>
                    <dt><strong><%= model_class.human_attribute_name(:content) %>:</strong></dt>
                    <dd><%= raw @tech_serv.content %></dd>
                </dl>
            </div>
            <div class="text-left block-mt">
                <%= link_to t('.edit', :default => t("helpers.links.edit")),
                edit_admin_tech_serv_path(@tech_serv), :class =>'btn btn-warning mr10'%>
                <%= link_to t('.back', :default => t("helpers.links.back")),
                admin_tech_servs_path, :class => 'btn btn-default'  %>
            </div>
        </div>
    </div>

</div>

