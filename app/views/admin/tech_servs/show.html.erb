<%- model_class = TechServ -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:avatar) %>:</strong></dt>
  <dd><img class="media-object" src="<%= @tech_serv.avatar %>" alt="Image"></dd>
  <dt><strong><%= model_class.human_attribute_name(:content) %>:</strong></dt>
  <dd><%= raw @tech_serv.content %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              admin_tech_servs_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_admin_tech_serv_path(@tech_serv), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              admin_tech_serv_path(@tech_serv),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
