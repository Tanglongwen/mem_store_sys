<%- model_class = GoodInstance -%>
<div class="container">
    <div class="panel">
        <div class="panel-heading">
            <div class="panel-title">商品实例详情</div>
        </div>
        <div class="panel-body">
            <div>
                <table class="table table-show-data">
                    <tr>
                        <th>
                            <%= model_class.human_attribute_name(:code) %>:
                        </th>
                        <td>
                            <%= @good_instance.code %>
                        </td>
                        <th>
                            <%= model_class.human_attribute_name(:good_id) %>:
                        </th>
                        <td>
                            <%= @good_instance.good_id %>
                        </td>
                    </tr>

                    <tr>
                        <th>
                            <%= model_class.human_attribute_name(:status) %>:
                        </th>
                        <td>
                            <%= @good_instance.status %>
                        </td>
                        <th>
                        </th>
                        <td>
                        </td>
                    </tr>
                </table>
            </div>
            <div class="text-left block-mt">
                <%= link_to t('.edit', :default => t("helpers.links.edit")),
                edit_admin_good_instance_path(@good_instance), :class => 'btn btn-warning mr10' %>
                <%= link_to t('.back', :default => t("helpers.links.back")),
                admin_good_instances_path, :class => 'btn btn-default mr10'  %>
            </div>
        </div>
    </div>

</div>

