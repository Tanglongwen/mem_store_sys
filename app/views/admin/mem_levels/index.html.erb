<%- model_class = MemLevel -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:code) %></th>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:level) %></th>
      <th><%= model_class.human_attribute_name(:score) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @mem_levels.each do |mem_level| %>
      <tr>
        <td><%= link_to mem_level.id, admin_mem_level_path(mem_level) %></td>
        <td><%= mem_level.code %></td>
        <td><%= mem_level.name %></td>
        <td><%= mem_level.level %></td>
        <td><%= mem_level.score %></td>
        <td><%= mem_level.created_at.strftime('%Y-%m-%d %H:%M:%S') %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      admin_mem_level_path(mem_level), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_admin_mem_level_path(mem_level), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      admin_mem_level_path(mem_level),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @mem_levels %>
<%= link_to t('.new', :default => t("helpers.links.new")),
            new_admin_mem_level_path,
            :class => 'btn btn-primary' %>
