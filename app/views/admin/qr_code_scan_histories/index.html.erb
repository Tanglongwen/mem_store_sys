<%- model_class = QrCodeScanHistory -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:user_id) %></th>
      <th><%= model_class.human_attribute_name(:good_id) %></th>
      <th><%= model_class.human_attribute_name(:good_instance_id) %></th>
      <th><%= model_class.human_attribute_name(:score) %></th>
      <th><%= model_class.human_attribute_name(:status) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @qr_code_scan_histories.each do |qr_code_scan_history| %>
      <tr>
        <td><%= link_to qr_code_scan_history.id, admin_qr_code_scan_history_path(qr_code_scan_history) %></td>
        <td><%= qr_code_scan_history.user_id %></td>
        <td><%= qr_code_scan_history.good_id %></td>
        <td><%= qr_code_scan_history.good_instance_id %></td>
        <td><%= qr_code_scan_history.score %></td>
        <td><%= qr_code_scan_history.status %></td>
        <td><%=l qr_code_scan_history.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      admin_qr_code_scan_history_path(qr_code_scan_history), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_admin_qr_code_scan_history_path(qr_code_scan_history), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      admin_qr_code_scan_history_path(qr_code_scan_history),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_admin_qr_code_scan_history_path,
            :class => 'btn btn-primary' %>
