<%- model_class = QrCodeScanHistory -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @qr_code_scan_history.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:good_id) %>:</strong></dt>
  <dd><%= @qr_code_scan_history.good_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:good_instance_id) %>:</strong></dt>
  <dd><%= @qr_code_scan_history.good_instance_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:score) %>:</strong></dt>
  <dd><%= @qr_code_scan_history.score %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @qr_code_scan_history.status %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              qr_code_scan_histories_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_qr_code_scan_history_path(@qr_code_scan_history), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              qr_code_scan_history_path(@qr_code_scan_history),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
