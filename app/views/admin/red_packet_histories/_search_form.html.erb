<%= form_tag("/admin/red_packet_histories", method: "get",class: "form-inline") do %>
  <%= label_tag(:catalog, "类型:") %>
  <%= text_field_tag(:catalog,@catalog,class: "form-control")  %>
  <%= label_tag(:start_date, "起始日期:") %>
  <input id='datetimepicker1' @start_date class='datetimepicker' name ='start_date'/>
<script>
    $("#datetimepicker1").datetimepicker();
</script>
  <%= label_tag(:end_date, "结束日期:") %>
  <input id='datetimepicker2' @end_date class='datetimepicker' name ='end_date' />
<script>
    $("#datetimepicker2").datetimepicker();
</script>
  <div class="form-group">
  <%= submit_tag("查 询",class: "btn btn-warning") %>
  </div>
<% end %>

