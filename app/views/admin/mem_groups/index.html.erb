<%- model_class = MemGroup -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:city) %></th>
      <th><%= model_class.human_attribute_name(:province) %></th>
      <th><%= model_class.human_attribute_name(:country) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @mem_groups.each do |mem_group| %>
      <tr>
        <td><%= link_to mem_group.id, admin_mem_group_path(mem_group) %></td>
        <td><%= mem_group.city %></td>
        <td><%= mem_group.province %></td>
        <td><%= mem_group.country %></td>
        <td><%= mem_group.created_at.strftime('%Y-%m-%d %H:%M:%S') %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      admin_mem_group_path(mem_group), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_admin_mem_group_path(mem_group), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      admin_mem_group_path(mem_group),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @mem_groups %>
<%= link_to t('.new', :default => t("helpers.links.new")),
            new_admin_mem_group_path,
            :class => 'btn btn-primary' %>
